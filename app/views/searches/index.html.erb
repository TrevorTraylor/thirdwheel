<h1>Searches#index</h1>
<p>Find me in app/views/searches/index.html.erb</p>
<% if @searchresult %>
<% @searchresult.each do |u|%>
<form action='/searches/enter'>
<input type='hidden' name='search' value="<%= u.id %>">
<input type='submit' value="<%= u.name %>">
<input name="authenticity_token" type="hidden" value="token_value">
</form>
<%end%>
<% end %>